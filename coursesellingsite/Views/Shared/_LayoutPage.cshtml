            <script>
            $(document).ready(function () {
                // When any 'Preview' button is clicked
                $('.show-preview').on('click', function () {
                    // Get the parent card that contains the clicked button
                    const card = $(this).closest('.preview-card');

                    // Extract data attributes from the card
                    const title = card.data('title');         // Course title
                    const desc = card.data('desc');           // Full description
                    const pic = card.data('pic');             // Image path
                    const price = card.data('price');         // Original price
                    const sell = card.data('sell');           // Selling price

                    // Update the preview section content with the card’s data
                    $('#preview-title').text(title);
                    $('#preview-desc').text(desc);
                    $('#preview-img').attr('src', pic);
                    $('#preview-price').text(price);
                    $('#preview-sell').text(sell);

                    // Show the preview section (remove "d-none" class)
                    $('#preview-section').removeClass('d-none');

                    // Scroll to the preview section smoothly
                    $('html, body').animate({
                        scrollTop: $('#preview-section').offset().top
                    }, 500);
                });
            });
        </script>
    




